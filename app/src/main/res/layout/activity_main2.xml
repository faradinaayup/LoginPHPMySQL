<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity"
    android:background="#89D6F3">

    <ImageButton
        android:id="@+id/park_elektro"
        android:layout_width="300dp"
        android:layout_height="158dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/park_komputer"
        app:layout_constraintVertical_bias="0.088"
        app:srcCompat="@drawable/park_biomed" />

    <ImageButton
        android:id="@+id/park_komputer"
        android:layout_width="300dp"
        android:layout_height="158dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209"
        app:srcCompat="@drawable/park_elektro" />

    <ImageButton
        android:id="@+id/park_biomed"
        android:layout_width="300dp"
        android:layout_height="158dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.291"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/park_elektro"
        app:layout_constraintVertical_bias="0.193"
        app:srcCompat="@drawable/park_elektro" />

    <TextView
        android:id="@+id/text_park_komputer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Departemen Teknik Komputer"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/park_komputer"
        app:layout_constraintEnd_toEndOf="@+id/park_komputer"
        app:layout_constraintStart_toStartOf="@+id/park_komputer"
        app:layout_constraintTop_toTopOf="@+id/park_komputer"
        app:layout_constraintVertical_bias="0.244" />

    <TextView
        android:id="@+id/text_park_elektro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Departemen Teknik Elektro"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/park_elektro"
        app:layout_constraintEnd_toEndOf="@+id/park_elektro"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="@+id/park_elektro"
        app:layout_constraintTop_toTopOf="@+id/park_elektro"
        app:layout_constraintVertical_bias="0.366" />

    <TextView
        android:id="@+id/text_park_biomed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Departemen Teknik Biomedik"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/park_biomed"
        app:layout_constraintEnd_toEndOf="@+id/park_biomed"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/park_biomed"
        app:layout_constraintTop_toTopOf="@+id/park_biomed"
        app:layout_constraintVertical_bias="0.511" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mau parkir dimana?"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059" />

    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/park_komputer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>